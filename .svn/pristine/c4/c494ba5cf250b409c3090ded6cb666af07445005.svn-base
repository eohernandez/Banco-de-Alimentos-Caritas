<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:comp="http://java.sun.com/jsf/composite/comp"
      xmlns:p="http://primefaces.org/ui">

    <ui:composition template="/template.xhtml">
        <ui:define name="title">
            <h:outputText value="#{bundle.SucursalesTitle}"></h:outputText>
        </ui:define>
        <ui:define name="actionTitle">
            <h:outputText value="#{bundle.Create}" />
        </ui:define>
        <ui:define name="displayDataSection">
            <h:panelGrid id="dataView" columns="4" styleClass="center">
                <p:outputLabel value="#{bundle.CreateSucursalesLabel_IDDonante}" for="IDDonante" />
                <h:panelGroup>
                    <p:selectOneMenu 
                        id="IDDonante" 
                        value="#{sucursalesController.selected.IDDonante}" 
                        required="true" 
                        requiredMessage="#{bundle.CreateSucursalesRequiredMessage_IDDonante}"  
                        filter="true"
                        filterMatchMode="contains"
                        maxlength="20"
                        >
                        <f:selectItem
                            itemDisabled="true"
                            itemLabel="Seleccione"
                            itemValue="#{null}"
                            />
                        <f:selectItems value="#{donantesController.getItemsAvailableSucursalesSelectOne(true)}" />
                    </p:selectOneMenu>
                    <p:message display="icon" for="IDDonante" />
                </h:panelGroup>
                <p:outputLabel value="#{bundle.CreateSucursalesLabel_IDSucursal}" for="IDSucursal" />
                <h:panelGroup>
                    <p:inputText
                        size="50" maxlength="15"
                        id="IDSucursal" 
                        value="#{sucursalesController.selected.IDSucursal}" 
                        required="true"
                        requiredMessage="#{bundle.CreateSucursalesRequiredMessage_IDSucursales}"
                        />
                    <p:message display="icon"
                               for="IDSucursal" />
                </h:panelGroup>
                <p:outputLabel value="#{bundle.CreateSucursalesLabel_direccion}" for="direccion" />
                <h:panelGroup>
                    <p:inputText
                        size="50"
                        id="direccion"
                        value="#{sucursalesController.selected.direccion}"
                        maxlength="50"
                        />
                    <p:message
                        display="icon"
                        for="direccion"
                        />
                </h:panelGroup>
                <p:outputLabel value="#{bundle.CreateSucursalesLabel_colonia}" for="colonia" />
                <h:panelGroup>
                    <p:inputText
                        size="50"
                        id="colonia"
                        value="#{sucursalesController.selected.colonia}"
                        maxlength="30"
                        />
                    <p:message display="icon" for="colonia" />
                </h:panelGroup>
                <p:outputLabel value="#{bundle.CreateSucursalesLabel_ciudad}" for="ciudad" />
                <h:panelGroup>
                    <p:inputText
                        size="50"
                        id="ciudad"
                        value="#{sucursalesController.selected.ciudad}"
                        maxlength="20"
                        />
                    <p:message display="icon" for="ciudad" />
                </h:panelGroup>
                <p:outputLabel value="#{bundle.CreateSucursalesLabel_estado}" for="estado" />
                <h:panelGroup>
                    <p:inputText
                        size="50"
                        id="estado"
                        value="#{sucursalesController.selected.estado}"
                        maxlength="10"
                        />
                    <p:message display="icon" for="estado" />
                </h:panelGroup>
                <p:outputLabel value="#{bundle.CreateSucursalesLabel_telefono}" for="telefono" />
                <h:panelGroup>
                    <p:inputText
                        size="50"
                        id="telefono"
                        value="#{sucursalesController.selected.telefono}"
                        maxlength="15"
                        />
                    <p:message display="icon" for="telefono" />
                </h:panelGroup>
                <p:outputLabel value="#{bundle.CreateSucursalesLabel_fax}" for="fax" />
                <h:panelGroup>
                    <p:inputText
                        size="50"
                        id="fax"
                        value="#{sucursalesController.selected.fax}"
                        maxlength="15"
                        />
                    <p:message display="icon" for="fax" />
                </h:panelGroup>
                <p:outputLabel value="#{bundle.CreateSucursalesLabel_contacto}" for="contacto" />
                <h:panelGroup>
                    <p:inputText
                        size="50"
                        id="contacto"
                        value="#{sucursalesController.selected.contacto}"
                        maxlength="50"
                        />
                    <p:message display="icon" for="contacto" />
                </h:panelGroup>
                <p:outputLabel value="#{bundle.CreateSucursalesLabel_mail}" for="mail" />
                <h:panelGroup>
                    <p:inputText
                        size="50"
                        id="mail"
                        value="#{sucursalesController.selected.mail}"
                        maxlength="30"
                        />
                    <p:message display="icon" for="mail" />
                </h:panelGroup>
            </h:panelGrid>
        </ui:define>
        <ui:define name="tools">
            <comp:createTools controller="#{sucursalesController}" />
        </ui:define>
    </ui:composition>

</html>
