<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core">

    <ui:composition template="/template.xhtml">
        <ui:define name="title">
            <h:outputText value="#{bundle.ListNivelAccesoTitle}"></h:outputText>
        </ui:define>
        <ui:define name="body">
            <h:form styleClass="jsfcrud_list_form">
                <h:panelGroup id="messagePanel" layout="block">
                    <h:messages errorStyle="color: red" infoStyle="color: green" layout="table"/>
                </h:panelGroup>
                <h:outputText escape="false" value="#{bundle.ListNivelAccesoEmpty}" rendered="#{nivelAccesoController.items.rowCount == 0}"/>
                <h:panelGroup rendered="#{nivelAccesoController.items.rowCount > 0}">
                    <h:outputText value="#{nivelAccesoController.pagination.pageFirstItem + 1}..#{nivelAccesoController.pagination.pageLastItem + 1}/#{nivelAccesoController.pagination.itemsCount}"/>&nbsp;
                    <h:commandLink action="#{nivelAccesoController.previous}" value="#{bundle.Previous} #{nivelAccesoController.pagination.pageSize}" rendered="#{nivelAccesoController.pagination.hasPreviousPage}"/>&nbsp;
                    <h:commandLink action="#{nivelAccesoController.next}" value="#{bundle.Next} #{nivelAccesoController.pagination.pageSize}" rendered="#{nivelAccesoController.pagination.hasNextPage}"/>&nbsp;
                    <h:dataTable value="#{nivelAccesoController.items}" var="item" border="0" cellpadding="2" cellspacing="0" rowClasses="jsfcrud_odd_row,jsfcrud_even_row" rules="all" style="border:solid 1px">
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListNivelAccesoTitle_IDNivel}"/>
                            </f:facet>
                            <h:outputText value="#{item.IDNivel}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListNivelAccesoTitle_nivel}"/>
                            </f:facet>
                            <h:outputText value="#{item.nivel}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="&nbsp;"/>
                            </f:facet>
                            <h:commandLink action="#{nivelAccesoController.prepareView}" value="#{bundle.ListNivelAccesoViewLink}"/>
                            <h:outputText value=" "/>
                            <h:commandLink action="#{nivelAccesoController.prepareEdit}" value="#{bundle.ListNivelAccesoEditLink}"/>
                            <h:outputText value=" "/>
                            <h:commandLink action="#{nivelAccesoController.destroy}" value="#{bundle.ListNivelAccesoDestroyLink}"/>
                        </h:column>
                    </h:dataTable>
                </h:panelGroup>
                <br />
                <h:commandLink action="#{nivelAccesoController.prepareCreate}" value="#{bundle.ListNivelAccesoCreateLink}"/>
                <br />
                <br />
                <h:commandLink value="#{bundle.ListNivelAccesoIndexLink}" action="/index" immediate="true" />
            </h:form>
        </ui:define>
    </ui:composition>

</html>
