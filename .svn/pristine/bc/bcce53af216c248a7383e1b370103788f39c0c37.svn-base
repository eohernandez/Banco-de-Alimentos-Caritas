<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core">

    <ui:composition template="/template.xhtml">
        <ui:define name="title">
            <h:outputText value="#{bundle.ListMovimientosTitle}"></h:outputText>
        </ui:define>
        <ui:define name="body">
            <h:form styleClass="jsfcrud_list_form">
                <h:panelGroup id="messagePanel" layout="block">
                    <h:messages errorStyle="color: red" infoStyle="color: green" layout="table"/>
                </h:panelGroup>
                <h:outputText escape="false" value="#{bundle.ListMovimientosEmpty}" rendered="#{movimientosController.items.rowCount == 0}"/>
                <h:panelGroup rendered="#{movimientosController.items.rowCount > 0}">
                    <h:outputText value="#{movimientosController.pagination.pageFirstItem + 1}..#{movimientosController.pagination.pageLastItem + 1}/#{movimientosController.pagination.itemsCount}"/>&nbsp;
                    <h:commandLink action="#{movimientosController.previous}" value="#{bundle.Previous} #{movimientosController.pagination.pageSize}" rendered="#{movimientosController.pagination.hasPreviousPage}"/>&nbsp;
                    <h:commandLink action="#{movimientosController.next}" value="#{bundle.Next} #{movimientosController.pagination.pageSize}" rendered="#{movimientosController.pagination.hasNextPage}"/>&nbsp;
                    <h:dataTable value="#{movimientosController.items}" var="item" border="0" cellpadding="2" cellspacing="0" rowClasses="jsfcrud_odd_row,jsfcrud_even_row" rules="all" style="border:solid 1px">
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListMovimientosTitle_IDFolio}"/>
                            </f:facet>
                            <h:outputText value="#{item.IDFolio}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListMovimientosTitle_tipoMov}"/>
                            </f:facet>
                            <h:outputText value="#{item.tipoMov}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListMovimientosTitle_fechaMov}"/>
                            </f:facet>
                            <h:outputText value="#{item.fechaMov}">
                                <f:convertDateTime pattern="MM/dd/yyyy HH:mm:ss" />
                            </h:outputText>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListMovimientosTitle_folioDon}"/>
                            </f:facet>
                            <h:outputText value="#{item.folioDon}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListMovimientosTitle_factura}"/>
                            </f:facet>
                            <h:outputText value="#{item.factura}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListMovimientosTitle_IDSucursal}"/>
                            </f:facet>
                            <h:outputText value="#{item.IDSucursal}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListMovimientosTitle_origen}"/>
                            </f:facet>
                            <h:outputText value="#{item.origen}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListMovimientosTitle_statusMov}"/>
                            </f:facet>
                            <h:outputText value="#{item.statusMov}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListMovimientosTitle_familias}"/>
                            </f:facet>
                            <h:outputText value="#{item.familias}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListMovimientosTitle_integrantes}"/>
                            </f:facet>
                            <h:outputText value="#{item.integrantes}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListMovimientosTitle_folioEntrada}"/>
                            </f:facet>
                            <h:outputText value="#{item.folioEntrada}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListMovimientosTitle_descuento}"/>
                            </f:facet>
                            <h:outputText value="#{item.descuento}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListMovimientosTitle_paquete}"/>
                            </f:facet>
                            <h:outputText value="#{item.paquete}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListMovimientosTitle_fechaSist}"/>
                            </f:facet>
                            <h:outputText value="#{item.fechaSist}">
                                <f:convertDateTime pattern="MM/dd/yyyy HH:mm:ss" />
                            </h:outputText>
                        </h:column>
                        <h:column>
