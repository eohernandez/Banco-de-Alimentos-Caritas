<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core">

    <ui:composition template="/template.xhtml">
        <ui:define name="title">
            <h:outputText value="#{bundle.CreateAccesosTitle}"></h:outputText>
        </ui:define>
        <ui:define name="body">
            <h:panelGroup id="messagePanel" layout="block">
                <h:messages errorStyle="color: red" infoStyle="color: green" layout="table"/>
            </h:panelGroup>
            <h:form>
                <h:panelGrid columns="2">
                    <h:outputLabel value="#{bundle.CreateAccesosLabel_subMenu}" for="subMenu" />
                    <h:inputText id="subMenu" value="#{accesosController.selected.subMenu}" title="#{bundle.CreateAccesosTitle_subMenu}" required="true" requiredMessage="#{bundle.CreateAccesosRequiredMessage_subMenu}"/>
                    <h:outputLabel value="#{bundle.CreateAccesosLabel_nuevo}" for="nuevo" />
                    <h:inputText id="nuevo" value="#{accesosController.selected.nuevo}" title="#{bundle.CreateAccesosTitle_nuevo}" required="true" requiredMessage="#{bundle.CreateAccesosRequiredMessage_nuevo}"/>
                    <h:outputLabel value="#{bundle.CreateAccesosLabel_editar}" for="editar" />
                    <h:inputText id="editar" value="#{accesosController.selected.editar}" title="#{bundle.CreateAccesosTitle_editar}" required="true" requiredMessage="#{bundle.CreateAccesosRequiredMessage_editar}"/>
                    <h:outputLabel value="#{bundle.CreateAccesosLabel_borrar}" for="borrar" />
                    <h:inputText id="borrar" value="#{accesosController.selected.borrar}" title="#{bundle.CreateAccesosTitle_borrar}" required="true" requiredMessage="#{bundle.CreateAccesosRequiredMessage_borrar}"/>
                    <h:outputLabel value="#{bundle.CreateAccesosLabel_grabar}" for="grabar" />
                    <h:inputText id="grabar" value="#{accesosController.selected.grabar}" title="#{bundle.CreateAccesosTitle_grabar}" required="true" requiredMessage="#{bundle.CreateAccesosRequiredMessage_grabar}"/>
                    <h:outputLabel value="#{bundle.CreateAccesosLabel_cancelar}" for="cancelar" />
                    <h:inputText id="cancelar" value="#{accesosController.selected.cancelar}" title="#{bundle.CreateAccesosTitle_cancelar}" required="true" requiredMessage="#{bundle.CreateAccesosRequiredMessage_cancelar}"/>
                    <h:outputLabel value="#{bundle.CreateAccesosLabel_imprimir}" for="imprimir" />
                    <h:inputText id="imprimir" value="#{accesosController.selected.imprimir}" title="#{bundle.CreateAccesosTitle_imprimir}" required="true" requiredMessage="#{bundle.CreateAccesosRequiredMessage_imprimir}"/>
                    <h:outputLabel value="#{bundle.CreateAccesosLabel_IDAccesos}" for="IDAccesos" />
                    <h:inputText id="IDAccesos" value="#{accesosController.selected.IDAccesos}" title="#{bundle.CreateAccesosTitle_IDAccesos}" required="true" requiredMessage="#{bundle.CreateAccesosRequiredMessage_IDAccesos}"/>
                    <h:outputLabel value="#{bundle.CreateAccesosLabel_IDNivel}" for="IDNivel" />
                    <h:selectOneMenu id="IDNivel" value="#{accesosController.selected.IDNivel}" title="#{bundle.CreateAccesosTitle_IDNivel}" >
                        <f:selectItems value="#{nivelAccesoController.itemsAvailableSelectOne}"/>
                    </h:selectOneMenu>
                    <h:outputLabel value="#{bundle.CreateAccesosLabel_IDForm}" for="IDForm" />
                    <h:selectOneMenu id="IDForm" value="#{accesosController.selected.IDForm}" title="#{bundle.CreateAccesosTitle_IDForm}" required="true" requiredMessage="#{bundle.CreateAccesosRequiredMessage_IDForm}">
                        <f:selectItems value="#{formulariosController.itemsAvailableSelectOne}"/>
                    </h:selectOneMenu>
                </h:panelGrid>
                <br />
                <h:commandLink action="#{accesosController.create}" value="#{bundle.CreateAccesosSaveLink}" />
                <br />
                <br />
                <h:commandLink action="#{accesosController.prepareList}" value="#{bundle.CreateAccesosShowAllLink}" immediate="true"/>
                <br />
                <br />
                <h:commandLink value="#{bundle.CreateAccesosIndexLink}" action="/index" immediate="true" />
            </h:form>
        </ui:define>
    </ui:composition>

</html>
