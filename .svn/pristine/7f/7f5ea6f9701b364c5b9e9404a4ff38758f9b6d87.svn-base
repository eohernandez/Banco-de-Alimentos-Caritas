<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core">

    <ui:composition template="/template.xhtml">
        <ui:define name="title">
            <h:outputText value="#{bundle.EditAccesoProgramaTitle}"></h:outputText>
        </ui:define>
        <ui:define name="body">
            <h:panelGroup id="messagePanel" layout="block">
                <h:messages errorStyle="color: red" infoStyle="color: green" layout="table"/>
            </h:panelGroup>
            <h:form>
                <h:panelGrid columns="2">
                    <h:outputLabel value="#{bundle.EditAccesoProgramaLabel_IDAccesoPrograma}" for="IDAccesoPrograma" />
                    <h:inputText id="IDAccesoPrograma" value="#{accesoProgramaController.selected.IDAccesoPrograma}" title="#{bundle.EditAccesoProgramaTitle_IDAccesoPrograma}" required="true" requiredMessage="#{bundle.EditAccesoProgramaRequiredMessage_IDAccesoPrograma}"/>
                    <h:outputLabel value="#{bundle.EditAccesoProgramaLabel_IDUsuario}" for="IDUsuario" />
                    <h:selectOneMenu id="IDUsuario" value="#{accesoProgramaController.selected.IDUsuario}" title="#{bundle.EditAccesoProgramaTitle_IDUsuario}" required="true" requiredMessage="#{bundle.EditAccesoProgramaRequiredMessage_IDUsuario}">
                        <f:selectItems value="#{usuariosController.itemsAvailableSelectOne}"/>
                    </h:selectOneMenu>
                    <h:outputLabel value="#{bundle.EditAccesoProgramaLabel_IDPrograma}" for="IDPrograma" />
                    <h:selectOneMenu id="IDPrograma" value="#{accesoProgramaController.selected.IDPrograma}" title="#{bundle.EditAccesoProgramaTitle_IDPrograma}" required="true" requiredMessage="#{bundle.EditAccesoProgramaRequiredMessage_IDPrograma}">
                        <f:selectItems value="#{programasController.itemsAvailableSelectOne}"/>
                    </h:selectOneMenu>
                    <h:outputLabel value="#{bundle.EditAccesoProgramaLabel_IDArea}" for="IDArea" />
                    <h:selectOneMenu id="IDArea" value="#{accesoProgramaController.selected.IDArea}" title="#{bundle.EditAccesoProgramaTitle_IDArea}" required="true" requiredMessage="#{bundle.EditAccesoProgramaRequiredMessage_IDArea}">
                        <f:selectItems value="#{areasController.itemsAvailableSelectOne}"/>
                    </h:selectOneMenu>
                </h:panelGrid>
                <h:commandLink action="#{accesoProgramaController.update}" value="#{bundle.EditAccesoProgramaSaveLink}"/>
                <br />
                <br />
                <h:commandLink action="View" value="#{bundle.EditAccesoProgramaViewLink}" immediate="true"/>
                <br />
                <h:commandLink action="#{accesoProgramaController.prepareList}" value="#{bundle.EditAccesoProgramaShowAllLink}" immediate="true"/>
                <br />
                <br />
                <h:commandLink value="#{bundle.EditAccesoProgramaIndexLink}" action="/index" immediate="true" />
            </h:form>
        </ui:define>
    </ui:composition>

</html>
