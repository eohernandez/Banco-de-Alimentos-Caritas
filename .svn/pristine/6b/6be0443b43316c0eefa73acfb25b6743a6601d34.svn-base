<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core">

    <ui:composition template="/template.xhtml">
        <ui:define name="title">
            <h:outputText value="#{bundle.ViewSubAreasTitle}"></h:outputText>
        </ui:define>
        <ui:define name="body">
            <h:panelGroup id="messagePanel" layout="block">
                <h:messages errorStyle="color: red" infoStyle="color: green" layout="table"/>
            </h:panelGroup>
            <h:form>
                <h:panelGrid columns="2">
                    <h:outputText value="#{bundle.ViewSubAreasLabel_IDSubArea}"/>
                    <h:outputText value="#{subAreasController.selected.IDSubArea}" title="#{bundle.ViewSubAreasTitle_IDSubArea}"/>
                    <h:outputText value="#{bundle.ViewSubAreasLabel_subArea}"/>
                    <h:outputText value="#{subAreasController.selected.subArea}" title="#{bundle.ViewSubAreasTitle_subArea}"/>
                    <h:outputText value="#{bundle.ViewSubAreasLabel_IDArea}"/>
                    <h:outputText value="#{subAreasController.selected.IDArea}" title="#{bundle.ViewSubAreasTitle_IDArea}"/>
                </h:panelGrid>
                <br />
                <h:commandLink action="#{subAreasController.destroyAndView}" value="#{bundle.ViewSubAreasDestroyLink}"/>
                <br />
                <br />
                <h:commandLink action="Edit" value="#{bundle.ViewSubAreasEditLink}"/>
                <br />
                <h:commandLink action="#{subAreasController.prepareCreate}" value="#{bundle.ViewSubAreasCreateLink}" />
                <br />
                <h:commandLink action="#{subAreasController.prepareList}" value="#{bundle.ViewSubAreasShowAllLink}"/>
                <br />
                <br />
                <h:commandLink value="#{bundle.ViewSubAreasIndexLink}" action="/index" immediate="true" />

            </h:form>
        </ui:define>
    </ui:composition>

</html>
