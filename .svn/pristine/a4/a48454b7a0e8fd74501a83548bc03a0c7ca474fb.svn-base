<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core">

    <ui:composition template="/template.xhtml">
        <ui:define name="title">
            <h:outputText value="#{bundle.ListReciboTitle}"></h:outputText>
        </ui:define>
        <ui:define name="body">
            <h:form styleClass="jsfcrud_list_form">
                <h:panelGroup id="messagePanel" layout="block">
                    <h:messages errorStyle="color: red" infoStyle="color: green" layout="table"/>
                </h:panelGroup>
                <h:outputText escape="false" value="#{bundle.ListReciboEmpty}" rendered="#{reciboController.items.rowCount == 0}"/>
                <h:panelGroup rendered="#{reciboController.items.rowCount > 0}">
                    <h:outputText value="#{reciboController.pagination.pageFirstItem + 1}..#{reciboController.pagination.pageLastItem + 1}/#{reciboController.pagination.itemsCount}"/>&nbsp;
                    <h:commandLink action="#{reciboController.previous}" value="#{bundle.Previous} #{reciboController.pagination.pageSize}" rendered="#{reciboController.pagination.hasPreviousPage}"/>&nbsp;
                    <h:commandLink action="#{reciboController.next}" value="#{bundle.Next} #{reciboController.pagination.pageSize}" rendered="#{reciboController.pagination.hasNextPage}"/>&nbsp;
                    <h:dataTable value="#{reciboController.items}" var="item" border="0" cellpadding="2" cellspacing="0" rowClasses="jsfcrud_odd_row,jsfcrud_even_row" rules="all" style="border:solid 1px">
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListReciboTitle_IDFolio}"/>
                            </f:facet>
                            <h:outputText value="#{item.IDFolio}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListReciboTitle_tipoMov}"/>
                            </f:facet>
                            <h:outputText value="#{item.tipoMov}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListReciboTitle_fechaMov}"/>
                            </f:facet>
                            <h:outputText value="#{item.fechaMov}">
                                <f:convertDateTime pattern="MM/dd/yyyy HH:mm:ss" />
                            </h:outputText>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListReciboTitle_folioDon}"/>
                            </f:facet>
                            <h:outputText value="#{item.folioDon}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListReciboTitle_factura}"/>
                            </f:facet>
                            <h:outputText value="#{item.factura}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListReciboTitle_IDSucursal}"/>
                            </f:facet>
                            <h:outputText value="#{item.IDSucursal}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListReciboTitle_origen}"/>
                            </f:facet>
                            <h:outputText value="#{item.origen}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListReciboTitle_statusMov}"/>
                            </f:facet>
                            <h:outputText value="#{item.statusMov}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListReciboTitle_familias}"/>
                            </f:facet>
                            <h:outputText value="#{item.familias}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListReciboTitle_integrantes}"/>
                            </f:facet>
                            <h:outputText value="#{item.integrantes}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListReciboTitle_folioEntrada}"/>
                            </f:facet>
                            <h:outputText value="#{item.folioEntrada}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListReciboTitle_descuento}"/>
                            </f:facet>
                            <h:outputText value="#{item.descuento}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListReciboTitle_paquete}"/>
                            </f:facet>
                            <h:outputText value="#{item.paquete}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListReciboTitle_fechaSist}"/>
                            </f:facet>
                            <h:outputText value="#{item.fechaSist}">
                                <f:convertDateTime pattern="MM/dd/yyyy HH:mm:ss" />
                            </h:outputText>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListReciboTitle_IDRecibo}"/>
                            </f:facet>
                            <h:outputText value="#{item.IDRecibo}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListReciboTitle_IDUsuario}"/>
                            </f:facet>
                            <h:outputText value="#{item.IDUsuario}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListReciboTitle_IDSucursales}"/>
                            </f:facet>
                            <h:outputText value="#{item.IDSucursales}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListReciboTitle_IDProveedor}"/>
                            </f:facet>
                            <h:outputText value="#{item.IDProveedor}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListReciboTitle_IDPrograma}"/>
                            </f:facet>
                            <h:outputText value="#{item.IDPrograma}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListReciboTitle_IDInstitucion}"/>
                            </f:facet>
                            <h:outputText value="#{item.IDInstitucion}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListReciboTitle_IDGrupoRef}"/>
                            </f:facet>
                            <h:outputText value="#{item.IDGrupoRef}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListReciboTitle_IDDonante}"/>
                            </f:facet>
                            <h:outputText value="#{item.IDDonante}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListReciboTitle_IDArea}"/>
                            </f:facet>
                            <h:outputText value="#{item.IDArea}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="&nbsp;"/>
                            </f:facet>
                            <h:commandLink action="#{reciboController.prepareView}" value="#{bundle.ListReciboViewLink}"/>
                            <h:outputText value=" "/>
                            <h:commandLink action="#{reciboController.prepareEdit}" value="#{bundle.ListReciboEditLink}"/>
                            <h:outputText value=" "/>
                            <h:commandLink action="#{reciboController.destroy}" value="#{bundle.ListReciboDestroyLink}"/>
                        </h:column>
                    </h:dataTable>
                </h:panelGroup>
                <br />
                <h:commandLink action="#{reciboController.prepareCreate}" value="#{bundle.ListReciboCreateLink}"/>
                <br />
                <br />
                <h:link outcome="/index" value="#{bundle.ListReciboIndexLink}"/>
            </h:form>
        </ui:define>
    </ui:composition>

</html>
