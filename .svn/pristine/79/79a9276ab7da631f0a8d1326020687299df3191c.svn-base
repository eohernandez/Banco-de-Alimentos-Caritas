<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core">

    <ui:composition template="/template.xhtml">
        <ui:define name="title">
            <h:outputText value="#{bundle.ListExistenciasTitle}"></h:outputText>
        </ui:define>
        <ui:define name="body">
            <h:form styleClass="jsfcrud_list_form">
                <h:panelGroup id="messagePanel" layout="block">
                    <h:messages errorStyle="color: red" infoStyle="color: green" layout="table"/>
                </h:panelGroup>
                <h:outputText escape="false" value="#{bundle.ListExistenciasEmpty}" rendered="#{existenciasController.items.rowCount == 0}"/>
                <h:panelGroup rendered="#{existenciasController.items.rowCount > 0}">
                    <h:outputText value="#{existenciasController.pagination.pageFirstItem + 1}..#{existenciasController.pagination.pageLastItem + 1}/#{existenciasController.pagination.itemsCount}"/>&nbsp;
                    <h:commandLink action="#{existenciasController.previous}" value="#{bundle.Previous} #{existenciasController.pagination.pageSize}" rendered="#{existenciasController.pagination.hasPreviousPage}"/>&nbsp;
                    <h:commandLink action="#{existenciasController.next}" value="#{bundle.Next} #{existenciasController.pagination.pageSize}" rendered="#{existenciasController.pagination.hasNextPage}"/>&nbsp;
                    <h:dataTable value="#{existenciasController.items}" var="item" border="0" cellpadding="2" cellspacing="0" rowClasses="jsfcrud_odd_row,jsfcrud_even_row" rules="all" style="border:solid 1px">
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListExistenciasTitle_fechaCad}"/>
                            </f:facet>
                            <h:outputText value="#{item.fechaCad}">
                                <f:convertDateTime pattern="MM/dd/yyyy HH:mm:ss" />
                            </h:outputText>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListExistenciasTitle_existencia}"/>
                            </f:facet>
                            <h:outputText value="#{item.existencia}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListExistenciasTitle_asignado}"/>
                            </f:facet>
                            <h:outputText value="#{item.asignado}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListExistenciasTitle_canPedido}"/>
                            </f:facet>
                            <h:outputText value="#{item.canPedido}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListExistenciasTitle_merma}"/>
                            </f:facet>
                            <h:outputText value="#{item.merma}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListExistenciasTitle_entrada}"/>
                            </f:facet>
                            <h:outputText value="#{item.entrada}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListExistenciasTitle_salida}"/>
                            </f:facet>
                            <h:outputText value="#{item.salida}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListExistenciasTitle_IDExistencias}"/>
                            </f:facet>
                            <h:outputText value="#{item.IDExistencias}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListExistenciasTitle_IDPrograma}"/>
                            </f:facet>
                            <h:outputText value="#{item.IDPrograma}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListExistenciasTitle_IDArticulo}"/>
                            </f:facet>
                            <h:outputText value="#{item.IDArticulo}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListExistenciasTitle_IDArea}"/>
                            </f:facet>
                            <h:outputText value="#{item.IDArea}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="&nbsp;"/>
                            </f:facet>
                            <h:commandLink action="#{existenciasController.prepareView}" value="#{bundle.ListExistenciasViewLink}"/>
                            <h:outputText value=" "/>
                            <h:commandLink action="#{existenciasController.prepareEdit}" value="#{bundle.ListExistenciasEditLink}"/>
                            <h:outputText value=" "/>
                            <h:commandLink action="#{existenciasController.destroy}" value="#{bundle.ListExistenciasDestroyLink}"/>
                        </h:column>
                    </h:dataTable>
                </h:panelGroup>
                <br />
                <h:commandLink action="#{existenciasController.prepareCreate}" value="#{bundle.ListExistenciasCreateLink}"/>
                <br />
                <br />
                <h:commandLink value="#{bundle.ListExistenciasIndexLink}" action="/index" immediate="true" />
            </h:form>
        </ui:define>
    </ui:composition>

</html>
