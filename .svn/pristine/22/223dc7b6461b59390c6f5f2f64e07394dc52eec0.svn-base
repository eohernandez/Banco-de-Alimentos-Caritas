#Cabecera_Caritas>img {
    height: 115px;
}

#Cabecera_Caritas_Login {
    margin: auto;
}

#Cabecera_Caritas {
    margin-top: 3.25em;
}


#topLogin, #middleLogin, #infoLogin {
    display: inline-block;
    width: 100%;
    margin: auto;
    position: relative;
    text-align: center;
    font-size: .75em;
    font-family: Verdana,Arial,sans-serif;
}

#middleLogin {
    top: .1em;
    z-index: 5;
    /*background: none;
    border: none;
    height: 6.55em;*/
}

#middleLogin > * {
    text-align: center;
    vertical-align: middle;
    line-height: 1em;
    left: 0em;
    right: 0em;
    /*color: #709302 !important;*/
}

#middleLogin > #titleLogin{
    font-size: 1.25em;
    padding: 1em 0em 0.25em 0em;
}

#middleLogin > #actionsLogin {
    font-size: 1em;
    padding: 0.1em 0em 1em 0em;
}

#infoLogin {
    left: 0em;
    background: #EEEEEE;
    overflow:auto;
    padding-top: .25em;
    min-height: 30em;
}

.ImprimeVale_Datos {
    margin: auto;
}

.ImprimeVale_Datos > label {
    vertical-align: super;
}

.barra .view
{
    display: inline-block;
    height: 19px;
    width: 15px;
    background: url("../images/file.png") repeat-x 0 0;
    margin-right:4px;
}
.barra .edit
{
    display: inline-block;
    height: 15px;
    width: 15px;
    background: url("../images/pencil.png") repeat-x 0 0;
    margin-right:4px;
}
.barra .destroy
{
    /*display: inline-block;*/
    height: 21px;
    width: 15px;
    background: url('../images/eliminiar.png') repeat-x 0 0;
}

.enlaces{
    display: inline-block;
    margin: 0 auto;
    list-style: none;
    overflow: hidden;
}
.enlaces li{
    padding: 0;
    margin: 0;
    padding-right: 10px;
    border-right: solid 1px blue;
    margin-right: 3px;
    margin-top: 12px;
    display: inline-block;
}
.enlaces li a{
    color:blue;
    font-weight: normal;
}
.enlaces li a:hover{
    /*cursor: pointer;*/
    text-decoration: underline;
    font-weight: normal;
}

