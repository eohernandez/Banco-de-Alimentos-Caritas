<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core">

    <ui:composition template="/template.xhtml">
        <ui:define name="title">
            <h:outputText value="#{bundle.ListDonativoEspTitle}"></h:outputText>
        </ui:define>
        <ui:define name="body">
            <h:form styleClass="jsfcrud_list_form">
                <h:panelGroup id="messagePanel" layout="block">
                    <h:messages errorStyle="color: red" infoStyle="color: green" layout="table"/>
                </h:panelGroup>
                <h:outputText escape="false" value="#{bundle.ListDonativoEspEmpty}" rendered="#{donativoEspController.items.rowCount == 0}"/>
                <h:panelGroup rendered="#{donativoEspController.items.rowCount > 0}">
                    <h:outputText value="#{donativoEspController.pagination.pageFirstItem + 1}..#{donativoEspController.pagination.pageLastItem + 1}/#{donativoEspController.pagination.itemsCount}"/>&nbsp;
                    <h:commandLink action="#{donativoEspController.previous}" value="#{bundle.Previous} #{donativoEspController.pagination.pageSize}" rendered="#{donativoEspController.pagination.hasPreviousPage}"/>&nbsp;
                    <h:commandLink action="#{donativoEspController.next}" value="#{bundle.Next} #{donativoEspController.pagination.pageSize}" rendered="#{donativoEspController.pagination.hasNextPage}"/>&nbsp;
                    <h:dataTable value="#{donativoEspController.items}" var="item" border="0" cellpadding="2" cellspacing="0" rowClasses="jsfcrud_odd_row,jsfcrud_even_row" rules="all" style="border:solid 1px">
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListDonativoEspTitle_IDFolio}"/>
                            </f:facet>
                            <h:outputText value="#{item.IDFolio}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListDonativoEspTitle_fecha}"/>
                            </f:facet>
                            <h:outputText value="#{item.fecha}">
                                <f:convertDateTime pattern="MM/dd/yyyy HH:mm:ss" />
                            </h:outputText>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListDonativoEspTitle_IDSucursal}"/>
                            </f:facet>
                            <h:outputText value="#{item.IDSucursal}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListDonativoEspTitle_contacto}"/>
                            </f:facet>
                            <h:outputText value="#{item.contacto}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListDonativoEspTitle_horarioAcopio}"/>
                            </f:facet>
                            <h:outputText value="#{item.horarioAcopio}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListDonativoEspTitle_horarioDescanso}"/>
                            </f:facet>
                            <h:outputText value="#{item.horarioDescanso}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListDonativoEspTitle_observaciones}"/>
                            </f:facet>
                            <h:outputText value="#{item.observaciones}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListDonativoEspTitle_IDDonante}"/>
                            </f:facet>
                            <h:outputText value="#{item.IDDonante}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="&nbsp;"/>
                            </f:facet>
                            <h:commandLink action="#{donativoEspController.prepareView}" value="#{bundle.ListDonativoEspViewLink}"/>
                            <h:outputText value=" "/>
                            <h:commandLink action="#{donativoEspController.prepareEdit}" value="#{bundle.ListDonativoEspEditLink}"/>
                            <h:outputText value=" "/>
                            <h:commandLink action="#{donativoEspController.destroy}" value="#{bundle.ListDonativoEspDestroyLink}"/>
                        </h:column>
                    </h:dataTable>
                </h:panelGroup>
                <br />
                <h:commandLink action="#{donativoEspController.prepareCreate}" value="#{bundle.ListDonativoEspCreateLink}"/>
                <br />
                <br />
                <h:commandLink value="#{bundle.ListDonativoEspIndexLink}" action="/index" immediate="true" />
            </h:form>
        </ui:define>
    </ui:composition>

</html>
