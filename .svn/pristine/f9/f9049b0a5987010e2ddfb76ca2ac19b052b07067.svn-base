<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:ui="http://java.sun.com/jsf/facelets"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:f="http://java.sun.com/jsf/core">

    <ui:composition template="/template.xhtml">
        <ui:define name="title">
            <h:outputText value="#{bundle.ListAccesosTitle}"></h:outputText>
        </ui:define>
        <ui:define name="body">
            <h:form styleClass="jsfcrud_list_form">
                <h:panelGroup id="messagePanel" layout="block">
                    <h:messages errorStyle="color: red" infoStyle="color: green" layout="table"/>
                </h:panelGroup>
                <h:outputText escape="false" value="#{bundle.ListAccesosEmpty}" rendered="#{accesosController.items.rowCount == 0}"/>
                <h:panelGroup rendered="#{accesosController.items.rowCount > 0}">
                    <h:outputText value="#{accesosController.pagination.pageFirstItem + 1}..#{accesosController.pagination.pageLastItem + 1}/#{accesosController.pagination.itemsCount}"/>&nbsp;
                    <h:commandLink action="#{accesosController.previous}" value="#{bundle.Previous} #{accesosController.pagination.pageSize}" rendered="#{accesosController.pagination.hasPreviousPage}"/>&nbsp;
                    <h:commandLink action="#{accesosController.next}" value="#{bundle.Next} #{accesosController.pagination.pageSize}" rendered="#{accesosController.pagination.hasNextPage}"/>&nbsp;
                    <h:dataTable value="#{accesosController.items}" var="item" border="0" cellpadding="2" cellspacing="0" rowClasses="jsfcrud_odd_row,jsfcrud_even_row" rules="all" style="border:solid 1px">
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListAccesosTitle_subMenu}"/>
                            </f:facet>
                            <h:outputText value="#{item.subMenu}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListAccesosTitle_nuevo}"/>
                            </f:facet>
                            <h:outputText value="#{item.nuevo}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListAccesosTitle_editar}"/>
                            </f:facet>
                            <h:outputText value="#{item.editar}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListAccesosTitle_borrar}"/>
                            </f:facet>
                            <h:outputText value="#{item.borrar}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListAccesosTitle_grabar}"/>
                            </f:facet>
                            <h:outputText value="#{item.grabar}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListAccesosTitle_cancelar}"/>
                            </f:facet>
                            <h:outputText value="#{item.cancelar}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListAccesosTitle_imprimir}"/>
                            </f:facet>
                            <h:outputText value="#{item.imprimir}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListAccesosTitle_IDAccesos}"/>
                            </f:facet>
                            <h:outputText value="#{item.IDAccesos}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListAccesosTitle_IDNivel}"/>
                            </f:facet>
                            <h:outputText value="#{item.IDNivel}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="#{bundle.ListAccesosTitle_IDForm}"/>
                            </f:facet>
                            <h:outputText value="#{item.IDForm}"/>
                        </h:column>
                        <h:column>
                            <f:facet name="header">
                                <h:outputText value="&nbsp;"/>
                            </f:facet>
                            <h:commandLink action="#{accesosController.prepareView}" value="#{bundle.ListAccesosViewLink}"/>
                            <h:outputText value=" "/>
                            <h:commandLink action="#{accesosController.prepareEdit}" value="#{bundle.ListAccesosEditLink}"/>
                            <h:outputText value=" "/>
                            <h:commandLink action="#{accesosController.destroy}" value="#{bundle.ListAccesosDestroyLink}"/>
                        </h:column>
                    </h:dataTable>
                </h:panelGroup>
                <br />
                <h:commandLink action="#{accesosController.prepareCreate}" value="#{bundle.ListAccesosCreateLink}"/>
                <br />
                <br />
                <h:commandLink value="#{bundle.ListAccesosIndexLink}" action="/index" immediate="true" />
            </h:form>
        </ui:define>
    </ui:composition>

</html>
